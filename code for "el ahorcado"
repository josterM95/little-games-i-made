        private void button2_Click(object sender, EventArgs e)
        {
            palabra = textBox1.Text;
            letra = textBox2.Text[0];
            char[] arrpalabra = new char[palabra.Length];
            arrpalabra = palabra.ToCharArray();
            
            for (int i = 0; i < arrpalabra.Length; i++)
            {
                if (letra == arrpalabra[i])
                {
                    if (i == 0)
                    {
                        label2.Visible = true;
                        label2.Text = Convert.ToString(arrpalabra[i]);
                        match++;
                    }
                    if (i == 1)
                    {
                        label3.Visible = true;
                        label3.Text = Convert.ToString(arrpalabra[i]);
                        match++;
                    }
                    if (i == 2)
                    {
                        label4.Visible = true;
                        label4.Text = Convert.ToString(arrpalabra[i]);
                        match++;
                    }
                    if (i == 3)
                    {
                        label5.Visible = true;
                        label5.Text = Convert.ToString(arrpalabra[i]);
                        match++;
                    }
                    if (i == 4)
                    {
                        label6.Visible = true;
                        label6.Text = Convert.ToString(arrpalabra[i]);
                        match++;
                    }
                    if (i == 5)
                    {
                        label7.Visible = true;
                        label7.Text = Convert.ToString(arrpalabra[i]);
                        match++;
                    }
                    if (i == 6)
                    {
                        label8.Visible = true;
                        label8.Text = Convert.ToString(arrpalabra[i]);
                        match++;
                    }
                    if (i == 7)
                    {
                        label9.Visible = true;
                        label9.Text = Convert.ToString(arrpalabra[i]);
                        match++;
                    }
                    if (i == 8)
                    {
                        label10.Visible = true;
                        label10.Text = Convert.ToString(arrpalabra[i]);
                        match++;
                    }
                    if (i == 9)
                    {
                        label11.Visible = true;
                        label11.Text = Convert.ToString(arrpalabra[i]);
                        match++;
                    }
                    if (i == 10)
                    {
                        label12.Visible = true;
                        label12.Text = Convert.ToString(arrpalabra[i]);
                        match++;
                    }
                    if (i == 11)
                    {
                        label13.Visible = true;
                        label13.Text = Convert.ToString(arrpalabra[i]);
                        match++;
                    }
                    if (i == 12)
                    {
                        label14.Visible = true;
                        label14.Text = Convert.ToString(arrpalabra[i]);
                        match++;
                    }
                    if (i == 13)
                    {
                        label15.Visible = true;
                        label15.Text = Convert.ToString(arrpalabra[i]);
                        match++;
                    }
                    if (i == 14)
                    {
                        label16.Visible = true;
                        label16.Text = Convert.ToString(arrpalabra[i]);
                        match++;
                    }
                    if (i == 15)
                    {
                        label17.Visible = true;
                        label17.Text = Convert.ToString(arrpalabra[i]);
                        match++;
                    }

                }
                
            }
            if (match == 0)
            {
                tries++;
                match = 0;
            }
            else
            {
                match = 0;
            }
                
            if (tries == 1)
            {
                pictureBox2.Visible = true;
                pictureBox3.Visible = false;
                pictureBox4.Visible = false;
                pictureBox5.Visible = false;
                pictureBox6.Visible = false;
                pictureBox7.Visible = false;


            }
            if (tries == 2)
            {
                pictureBox3.Visible = true;
                pictureBox2.Visible = false;
                pictureBox4.Visible = false;
                pictureBox5.Visible = false;
                pictureBox6.Visible = false;
                pictureBox7.Visible = false;
            }
            if (tries == 3)
            {
                pictureBox4.Visible = true;
                pictureBox3.Visible = false;
                pictureBox2.Visible = false;
                pictureBox5.Visible = false;
                pictureBox6.Visible = false;
                pictureBox7.Visible = false;
            }
            if (tries == 4)
            {
                pictureBox5.Visible = true;
                pictureBox3.Visible = false;
                pictureBox4.Visible = false;
                pictureBox2.Visible = false;
                pictureBox6.Visible = false;
                pictureBox7.Visible = false;
            }
            if (tries == 5)
            {
                pictureBox6.Visible = true;
                pictureBox3.Visible = false;
                pictureBox4.Visible = false;
                pictureBox5.Visible = false;
                pictureBox2.Visible = false;
                pictureBox7.Visible = false;
            }
            if (tries >= 6)
            {
                pictureBox7.Visible = true;
                pictureBox3.Visible = false;
                pictureBox4.Visible = false;
                pictureBox5.Visible = false;
                pictureBox6.Visible = false;
                pictureBox2.Visible = false;
                MessageBox.Show("has sido ahorcado");
            }
            textBox2.Clear();
            textBox2.Focus();
        }
    }
